#include <iostream>
#include <string>

using namespace std;  
int main() {
    string text = "hello world";
    int count = 0;
    
    for (int i = 0; i < text.length(); i++) {
    char c = text[i];
    if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
        c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U') {
        count++;
    }
}

    cout << "Number of vowels: " << count << endl;
    return 0;
}
