#include <iostream>
#include <string>

using namespace std;  
int main() {
    string text = "hello world";
    int count = 0;
    
    for (char c : text) {
        if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
            c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U') {
            count++;
        }
    }

    // Output using cout without std::
    cout << "Number of vowels: " << count << endl;
    return 0;
}
